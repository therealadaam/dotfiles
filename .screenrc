hardstatus on
hardstatus alwayslastline "%?%{wk}%-Lw%?%{yK}%n*%f %t%?(%u)%?%?%{wk}%+Lw%? %{gk}%=%c %{yk}%d/%M/%Y"
escape ^Jj
startup_message off
bind ' ' windowlist -b
shell -${SHELL}
altscreen on
vbell on
screen -t "Local" 1
screen -t "Chef" 2
screen -t "Cent" 3
screen -t "Ubnt" 4
screen -t "Suse" 5
